@model IEnumerable<MUE.Web.EntitiesDTO.MUEDTO.TypeOfServiceDTO>

@{
    ViewBag.Title = "Index";
}

<h2>Список услуг</h2>

<p>
    @Html.ActionLink("Создать", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TypeOfServiceId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UnitOfMeasurment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsMeter)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TypeOfServiceId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UnitOfMeasurment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsMeter)
        </td>
        <td>
            @Html.ActionLink("Удалить", "Delete", new { id=item.TypeOfServiceId })
        </td>
    </tr>
}

</table>
