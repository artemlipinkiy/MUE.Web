@model IEnumerable<MUE.Web.EntitiesDTO.MUEDTO.TariffDTO>

@{
    ViewBag.Title = "Index";
}

<h2>Тарифы</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TariffId)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Flat)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeOfService)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeOfServiceId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Value)
        </th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.TariffId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Flat)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TypeOfService)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TypeOfServiceId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Value)
    </td>
</tr>
}

</table>
